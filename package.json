{
  "name": "properproperties",
  "version": "1.0.0",
  "description": "App for managing properties",
  "main": "src/index.js",
  "scripts": {
    "start": "electron ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.properproperties",
    "productName": "Proper Properties",
    "target": "NSIS",
    "files": [
      "src/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  },
  "_moduleAliases": {
    "@components": "src/components",
    "@js": "src/js"
  },
  "repository": "https://github.com/DavidLeDuy/ProperProperties",
  "keywords": [
    "electron",
    "properties",
    "css",
    "html",
    "js"
  ],
  "author": "Kurt Azad, David Le Duy, Lindner Sascha, Temiz Ekrem, Zauner Sascha",
  "license": "ISC",
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mssql": "^6.2.0",
    "yarn": "^1.22.4"
  }
}
